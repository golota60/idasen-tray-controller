{
  "build": {
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "idasen-tray-webview",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "window": {
        "close": true
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": ["icons/carrot.png"],
      "identifier": "szywis.idasen.tray-v0.0.1",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "Idasen Controller",
        "label": "main",
        "width": 800,
        "alwaysOnTop": true
      }
    ],
    "systemTray": {
      "iconPath": "icons/carrot.png",
      "iconAsTemplate": true
    }
  }
}
